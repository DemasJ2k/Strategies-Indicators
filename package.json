{
  "name": "market-playbook-agent",
  "version": "1.0.0",
  "description": "AI-powered Market Playbook Classifier Agent",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:scenarios": "ts-node -r tsconfig-paths/register tests/runScenarios.ts",
    "sim:csv": "ts-node -r tsconfig-paths/register src/runners/runSimulatorCSV.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "trading",
    "agent",
    "classifier",
    "playbook"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "socket.io": "^4.8.1"
  }
}
